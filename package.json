{
  "name": "next-parallel-routes",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "npx @cloudflare/next-on-pages --experimental-minify",
    "pages:build-deploy": "pnpm run pages:build && pnpm run pages:deploy",
    "pages:deploy": "wrangler pages deploy .vercel/output/static",
    "pages:watch": "npx @cloudflare/next-on-pages --watch",
    "pages:dev": "npx wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.3",
    "@types/node": "18.15.11",
    "@types/react": "18.2.9",
    "@types/react-dom": "18.2.4",
    "class-variance-authority": "^0.5.2",
    "clsx": "^1.2.1",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "lucide-react": "^0.161.0",
    "next": "13.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remarkable": "^2.0.1",
    "tailwind-merge": "^1.12.0",
    "tailwindcss-animate": "^1.0.5"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^0.10.1",
    "@types/remarkable": "^2.0.3",
    "autoprefixer": "10.4.14",
    "postcss": "8.4.22",
    "tailwindcss": "3.3.1",
    "typescript": "5.1.3",
    "vercel": "^30.2.1",
    "wrangler": "^3.1.0"
  }
}
